package kds;

proto_go_package="github.com/iakud/krocher/kds/kdsc/example/kdspb";

import "example1.kds";

entity City {
	int64 PlayerId = 1;
	PlayerBasicInfo PlayerBasicInfo = 2;
	CityBaseInfo CityInfo = 3;
}

component CityBaseInfo {
	repeated Vector Positions = 1;
	map<int32, empty> Troops = 2;
	bytes BuildInfo = 3;
}

component Vector {
	int32 X = 1;
	int32 Y = 2;
}